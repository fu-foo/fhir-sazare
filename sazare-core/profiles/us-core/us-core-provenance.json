        "path": "Provenance.agent.type",
        "min": 0,
        "max": "1",
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "http://hl7.org/fhir/us/core/ValueSet/us-core-provenance-participant-type"
