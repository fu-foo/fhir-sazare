  "description": "The US Core Encounter Profile meets the U.S. Core Data for Interoperability (USCDI) 'Encounter' requirements. This profile sets minimum expectations for the Encounter resource to record, search, and fetch basic encounter information for a patient.",
  "copyright": "Used by permission of HL7 International, all rights reserved Creative Commons License",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Encounter",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Encounter",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Encounter",
        "path": "Encounter",
        "mustSupport": false
      },
      {
        "id": "Encounter.identifier",
        "path": "Encounter.identifier",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Encounter.status",
        "path": "Encounter.status",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Encounter.class",
        "path": "Encounter.class",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Encounter.type",
        "path": "Encounter.type",
        "min": 1,
        "max": "*",
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "http://hl7.org/fhir/us/core/ValueSet/us-core-encounter-type"
        }
      },
      {
        "id": "Encounter.subject",
        "path": "Encounter.subject",
        "min": 1,
        "max": "1",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/core/StructureDefinition/us-core-patient"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Encounter.participant",
        "path": "Encounter.participant",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Encounter.period",
        "path": "Encounter.period",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Encounter.reasonCode",
        "path": "Encounter.reasonCode",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Encounter.hospitalization",
        "path": "Encounter.hospitalization",
        "min": 0,
        "max": "1",
        "mustSupport": false
      },
      {
        "id": "Encounter.hospitalization.dischargeDisposition",
        "path": "Encounter.hospitalization.dischargeDisposition",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Encounter.location",
        "path": "Encounter.location",
        "min": 0,
        "max": "*",
        "mustSupport": true
      }
