{
  "resourceType": "StructureDefinition",
  "id": "us-core-pulse-oximetry",
  "url": "http://hl7.org/fhir/us/core/StructureDefinition/us-core-pulse-oximetry",
  "version": "5.0.1",
  "name": "USCorePulseOximetryProfile",
  "title": "US Core Pulse Oximetry Profile",
  "status": "active",
  "experimental": false,
  "date": "2022-04-20",
  "publisher": "HL7 International - Cross-Group Projects",
  "contact": [
    {
      "name": "HL7 International - Cross-Group Projects",
      "telecom": [
        {
          "system": "url",
          "value": "http://www.hl7.org/Special/committees/cgp"
        }
      ]
    }
  ],
  "description": "The US Core Pulse Oximetry Profile meets the U.S. Core Data for Interoperability (USCDI) requirements. This profile sets minimum expectations for the Observation resource to record, search, and fetch pulse oximetry observations.",
  "copyright": "Used by permission of HL7 International, all rights reserved Creative Commons License",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Observation",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/vitalsigns",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Observation",
        "path": "Observation",
        "mustSupport": false
      },
      {
        "id": "Observation.code",
        "path": "Observation.code",
        "min": 1,
        "max": "1",
        "patternCodeableConcept": {
          "coding": [
            {
              "system": "http://loinc.org",
              "code": "59408-5"
            }
          ]
        },
        "mustSupport": true
      },
      {
        "id": "Observation.subject",
        "path": "Observation.subject",
        "min": 1,
        "max": "1",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/us/core/StructureDefinition/us-core-patient"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.value[x]",
        "path": "Observation.value[x]",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Quantity"
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.component",
        "path": "Observation.component",
        "slicing": {
          "discriminator": [
            {
              "type": "pattern",
              "path": "code"
            }
          ],
          "rules": "open"
        },
        "min": 0,
        "max": "*",
        "mustSupport": false
      },
      {
        "id": "Observation.component:flowRate",
        "path": "Observation.component",
        "sliceName": "flowRate",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Observation.component:concentration",
        "path": "Observation.component",
        "sliceName": "concentration",
        "min": 0,
        "max": "1",
        "mustSupport": true
      }
    ]
  }
}
